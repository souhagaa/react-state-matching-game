{
  "name": "game",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "gh-pages": "^3.1.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "homepage": "https://souhagaa.github.io/react-state-matching-game",
  "scripts": {
    "start": "webpack-dev-server --open --mode development --env.entryDir=src",
    "start-refactor": "webpack-dev-server --open --mode development --env.entryDir=refactor",
    "build": "webpack --mode development --config webpack.config.js",
    "test": "jest",
    "test:module-2": "jest test/module-2/",
    "test:module-3": "jest test/module-3/",
    "test:module-4": "jest test/module-4/",
    "test:module-5": "jest test/module-5/",
    "deploy" : "gh-pages -b master -d build",
    "predeploy": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.8.3",
    "@testing-library/react": "^10.0.2",
    "babel-loader": "^8.2.2",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "file-loader": "^5.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "jest-enzyme": "^7.1.2",
    "jest-expect-message": "^1.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js",
      "jest-expect-message"
    ],
    "testPathIgnorePatterns": [
      "<tootDir>/node_modules/"
    ]
  }
}
